{
  "name": "ergebnis/php-cs-fixer-config-template",
  "description": "Provides a configuration factory and multiple rule sets for friendsofphp/php-cs-fixer.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/php-cs-fixer-config-template",
  "support": {
    "issues": "https://github.com/ergebnis/php-cs-fixer-config-template/issues",
    "source": "https://github.com/ergebnis/php-cs-fixer-config-template",
    "security": "https://github.com/ergebnis/php-cs-fixer-config-template/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "friendsofphp/php-cs-fixer": "~3.29.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.37.0",
    "ergebnis/data-provider": "^3.0.0",
    "ergebnis/license": "^2.2.0",
    "ergebnis/phpunit-slow-test-detector": "^2.3.0",
    "fakerphp/faker": "^1.23.0",
    "phpunit/phpunit": "^10.3.5",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.4",
    "vimeo/psalm": "^5.15.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\Test\\": "test"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.20"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
