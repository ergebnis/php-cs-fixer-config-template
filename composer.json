{
  "name": "ergebnis/php-cs-fixer-config-template",
  "description": "Provides a GitHub repository template for a configuration factory and rule set factories for friendsofphp/php-cs-fixer.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/php-cs-fixer-config-template",
  "support": {
    "issues": "https://github.com/ergebnis/php-cs-fixer-config-template/issues",
    "source": "https://github.com/ergebnis/php-cs-fixer-config-template",
    "security": "https://github.com/ergebnis/php-cs-fixer-config-template/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "friendsofphp/php-cs-fixer": "~3.70.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.45.0",
    "ergebnis/data-provider": "^3.4.0",
    "ergebnis/license": "^2.6.0",
    "ergebnis/phpstan-rules": "^2.8.0",
    "ergebnis/phpunit-slow-test-detector": "^2.19.0",
    "ergebnis/rector-rules": "^1.3.0",
    "fakerphp/faker": "^1.24.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.6",
    "phpstan/phpstan-deprecation-rules": "^2.0.1",
    "phpstan/phpstan-phpunit": "^2.0.4",
    "phpstan/phpstan-strict-rules": "^2.0.3",
    "phpunit/phpunit": "^10.5.26",
    "rector/rector": "^2.0.9",
    "roave/backward-compatibility-check": "^8.6.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\Test\\": "test"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    },
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
