{
  "name": "ergebnis/php-cs-fixer-config-template",
  "type": "library",
  "description": "Provides a configuration factory and multiple rule sets for friendsofphp/php-cs-fixer.",
  "homepage": "https://github.com/ergebnis/php-cs-fixer-config-template",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "friendsofphp/php-cs-fixer": "~2.17.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.11.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.4.0",
    "infection/infection": "~0.15.3",
    "phpstan/extension-installer": "^1.0.5",
    "phpstan/phpstan": "~0.12.59",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpstan/phpstan-strict-rules": "~0.12.7",
    "phpunit/phpunit": "^8.5.13",
    "psalm/plugin-phpunit": "~0.12.2",
    "vimeo/psalm": "^3.18"
  },
  "config": {
    "platform": {
      "php": "7.2.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\Test\\": "test"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/php-cs-fixer-config-template/issues",
    "source": "https://github.com/ergebnis/php-cs-fixer-config-template"
  }
}
